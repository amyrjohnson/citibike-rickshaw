<h1>Tic Tac Toe</h1>
<h2></h2>
<!-- Maybe start out on index page with intro and send to a /play page? -->
<body>

<form action="/play" method="post">
  <input type="Submit" value="Start">
</form>

<div id="board">
    <table>
      <tr id="row1">
        <td class="square">0</td>
        <td class="square v">2</td>
        <td class="square">3</td>
      </tr>
      <tr id="row2">
        <td class="square h">4</td>
        <td class="square v h">5</td>
        <td class="square h">6</td>
      </tr>
      <tr id="row3">
        <td class="square">7</td>
        <td class="square v">8</td>
        <td class="square">9</td>
      </tr>
    </table>
</div>

</body>