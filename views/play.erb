<h1>Tic Tac Toe</h1>

<body>

<h2>Choose an available square by number<h2>
<form action="/move" method="post">
  <input type="text" name="move">
  <input type="Submit" value="play">
</form>

<div id="board">
    <table>
      <tr id="row1">
        <td class="square">0</td>
        <td class="square v">0</td>
        <td class="square">0</td>
      </tr>
      <tr id="row2">
        <td class="square h">0</td>
        <td class="square v h">0</td>
        <td class="square h">0</td>
      </tr>
      <tr id="row3">
        <td class="square">0</td>
        <td class="square v">0</td>
        <td class="square">0</td>
      </tr>
    </table>
</div>

</body>